<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <Class name="org.microbean.configuration.spi.TypeLiteral"/>
    <Or>
      <Method name="equals"/>
      <Method name="hashCode"/>
    </Or>
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
  </Match>
  <Match>
    <Class name="org.microbean.configuration.Configurations"/>
    <Bug pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS"/>
  </Match>
</FindBugsFilter>
